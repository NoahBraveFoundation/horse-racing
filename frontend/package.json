{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "schema": "curl -X POST -H \"Content-Type: application/json\" -d @introspection-query.json http://localhost:8080/graphql > schema.json",
    "schema:sdl": "npx get-graphql-schema http://localhost:8080/graphql > schema.graphql",
    "relay": "npm run schema:sdl && relay-compiler"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/graphql": "^14.2.3",
    "graphql": "^16.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-relay": "^20.1.1",
    "react-router-dom": "^7.8.1",
    "relay-runtime": "^20.1.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.2",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-relay": "^20.1.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "get-graphql-schema": "^2.1.2",
    "globals": "^16.3.0",
    "relay-compiler": "^20.1.1",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
